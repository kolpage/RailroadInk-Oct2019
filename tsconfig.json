{ 
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "removeComments": false,
      "noImplicitAny": false,
      "suppressImplicitAnyIndexErrors": true ,
      "outDir": "dist",
      "jsx": "react"
    },
    "exclude": [ "node_modules", "**/*.spec.ts" ] 
  }